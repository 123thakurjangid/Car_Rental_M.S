@model Car_Rental.Business.Model.UpdateCarModel

<div class="row">
    <div class="column">
        <h2 class="formH text-dark">Manage Car</h2>
        <form class="CustomerForm" asp-action="UpdateCar" asp-controller="Admin" method="post">

            <label>Car Id</label><br />
            <input class="col-sm-9 car_Updt_inp" asp-for="Car_Id" readonly /><br />
      

            <label>Car Plate Number</label><br />
            <input class="col-sm-9 car_Updt_inp" asp-for="Plate_Number" /><br />

            <label>Brand</label><br />
            <input class="col-sm-9 car_Updt_inp" type="text" asp-for="Company" /><br />

            <label>Model</label><br />
            <input class="col-sm-9 car_Updt_inp" type="text" asp-for="Model" /> <br />

            <label>Price</label><br />
            <input class="col-sm-9 car_Updt_inp" type="text" asp-for="Price" /> <br />

            <label>Color</label><br />
            <input class="col-sm-9 car_Updt_inp" type="text" asp-for="Color" /> <br />

            <label>Available</label><br />
            <input class="col-sm-9 car_Updt_inp" type="text" asp-for="Available" /><br />

            <input class="col-sm-9 btn btn-outline-warning" type="submit" value="Update" />

        </form>
    </div>
    <div class="column">
        <img id="rightcolimg" src="~/AdminPanelCars/Mustang Gt.png" />
    </div>
</div>
